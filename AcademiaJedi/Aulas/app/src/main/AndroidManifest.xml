        <activity
            android:name=".ui.GroupingDialogActivity"
            android:label="@string/title_activity_grouping_dialog" >
        </activity>
        <activity
            android:name=".ui.ManagerActivity"
            android:label="@string/title_activity_manager" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.component.FragmentAndTabsActivity"
            android:label="@string/title_activity_fragment_and_tabs" >
        </activity>
    </application>

</manifest>
